{
  "name": "kodereceh/esisma-api",
  "description": "RESTful API for e-SisMA Nagari App",
  "keywords": [
    "esisma",
    "minang",
    "minang kabau",
    "nagari",
    "surat",
    "dokumen",
    "pengarsipan",
    "arsip"
  ],
  "license": "MIT",
  "type": "project",
  "require": {
    "php": ">=7.1.3",
    "laravel/lumen-framework": "5.6.*",
    "vlucas/phpdotenv": "~2.2",
    "flipbox/lumen-generator": "^5.6",
    "symfony/config": "^4.1",
    "league/flysystem": "^1.0",
    "barryvdh/laravel-cors": "^0.11.2",
    "phpoffice/phpword": "dev-master",
    "chillerlan/php-qrcode": "^3.0",
    "dfridrich/php-mime-type": "^2.0"
  },
  "require-dev": {
    "fzaninotto/faker": "~1.4",
    "phpunit/phpunit": "~7.0",
    "mockery/mockery": "~1.0"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    },
    "classmap": [
      "database/",
      "app/Libraries"
    ]
  },
  "autoload-dev": {
    "classmap": [
      "tests/",
      "database/"
    ]
  },
  "scripts": {
    "post-root-package-install": [
      "php -r \"copy('.env.example', '.env');\""
    ]
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "optimize-autoloader": true
  },
  "repositories": {
    
  }
}
